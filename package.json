{
  "name": "@theholocron/react-template",
  "version": "0.2.3",
  "description": "<description>",
  "homepage": "https://github.com/theholocron/react-template#readme",
  "bugs": "https://github.com/theholocron/react-template/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/theholocron/react-template.git"
  },
  "license": "GPL-3.0",
  "author": "Newton Koumantzelis",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react-template.es.js",
      "require": "./dist/react-template.cjs.js"
    },
    "./style": "./dist/style.css"
  },
  "main": "dist/react-template.cjs.js",
  "module": "dist/react-template.es.js",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build",
    "dev": "vite",
    "lint": "docker run -e LOG_LEVEL=DEBUG -e RUN_LOCAL=true -v .:/tmp/lint --rm ghcr.io/super-linter/super-linter:latest",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "storybook dev -p 6006",
    "test": "vitest",
    "test:cypress": "cypress open",
    "test:storybook": "vitest --project=storybook"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@codesandbox/storybook-addon": "^0.2.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@storybook/addon-a11y": "^9.0.18",
    "@storybook/addon-coverage": "^2.0.0",
    "@storybook/addon-designs": "^10.0.2",
    "@storybook/addon-docs": "^9.0.18",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/addon-themes": "^9.0.18",
    "@storybook/addon-vitest": "^9.0.18",
    "@storybook/react": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@theholocron/commitlint-config": "^4.1.0",
    "@theholocron/eslint-config": "^4.1.0",
    "@theholocron/lint-staged-config": "^4.1.0",
    "@theholocron/prettier-config": "^4.1.0",
    "@theholocron/stylelint-config": "^4.0.0",
    "@tsconfig/node-lts": "^22.0.2",
    "@tsconfig/vite-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "alex": "^11.0.1",
    "axe-playwright": "^2.1.0",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-plugin-cypress": "^5.1.0",
    "eslint-plugin-storybook": "^9.0.18",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "msw-storybook-addon": "^2.0.5",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sort-package-json": "^3.4.0",
    "storybook": "^9.0.18",
    "storybook-addon-pseudo-states": "^9.0.18",
    "tsc-files": "^1.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "packageManager": "npm@9.5.0",
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/**/*.css",
        "maxSize": "50 kB"
      },
      {
        "path": "./dist/**/*.js",
        "maxSize": "100 kB"
      }
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "releases": "https://github.com/theholocron/react-template/releases",
  "wiki": "https://github.com/theholocron/react-template/wiki"
}
