{
  "name": "@theholocron/react-template",
  "description": "<description>",
  "homepage": "https://github.com/theholocron/react-template#readme",
  "author": "Newton Koumantzelis",
  "main": "dist/react-template.cjs.js",
  "module": "dist/react-template.es.js",
  "style": "dist/style.css",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-template.es.js",
      "require": "./dist/react-template.cjs.js"
    },
    "./style": "./dist/style.css"
  },
  "files": [
    "dist/*"
  ],
  "type": "module",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theholocron/react-template.git"
  },
  "bugs": "https://github.com/theholocron/react-template/issues",
  "releases": "https://github.com/theholocron/react-template/releases",
  "wiki": "https://github.com/theholocron/react-template/wiki",
  "license": "GPL-3.0",
  "packageManager": "npm@9.5.0",
  "bundlewatch": {
    "files": [
      {
        "path": "./dist/**/*.css",
        "maxSize": "50 kB"
      },
      {
        "path": "./dist/**/*.js",
        "maxSize": "100 kB"
      }
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:storybook": "storybook build",
    "lint": "docker run -e LOG_LEVEL=DEBUG -e RUN_LOCAL=true -v .:/tmp/lint --rm ghcr.io/super-linter/super-linter:latest",
    "preview": "vite preview",
    "release": "changeset",
    "start": "storybook dev -p 6006",
    "test": "vitest",
    "test:cypress": "cypress open",
    "test:storybook": "test-storybook --coverage"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@theholocron/commitlint-config": "^3.2.1",
    "@theholocron/eslint-config": "^3.2.1",
    "@theholocron/jest-config": "^3.2.1",
    "@theholocron/lint-staged-config": "^3.2.1",
    "@theholocron/prettier-config": "^3.2.1",
    "@theholocron/storybook-config": "^3.2.1",
    "@tsconfig/vite-react": "^3.0.2",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.3",
    "cypress": "^13.15.0",
    "editorconfig-checker": "^6.0.0",
    "jsdom": "^25.0.1",
    "msw": "^2.4.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-syntax-highlighter": "^15.6.1",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
