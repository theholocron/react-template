{
  "display": "React Template",
  "extends": "@tsconfig/vite-react/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@/package": ["./package.json"],
      "@/*": ["./src/*"]
